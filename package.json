{
  "name": "super-detype",
  "version": "2.1.7",
  "description": "This package converts your WHOLE Typescript project to Javascript",
  "type": "module",
  "homepage": "https://super-detype.superkenil.com",
  "scripts": {
    "test": "bun run ./dist/index.js test/input test/output",
    "test:src": "bun run ./src/index.ts test/input test/output",
    "build": "esbuild ./src/index.ts --outfile=./dist/index.js --platform=node --bundle --minify --format=esm --packages=external && tsc",
    "release": "npm run build && npm publish"
  },
  "displayName": "super-detype",
  "maintainers": [
    "Super-Kenil"
  ],
  "main": "./dist/index.js",
  "bin": {
    "super-detype": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "converter",
    "typescript",
    "typescript-to-javascript",
    "compiler"
  ],
  "engines": {
    "node": ">=22"
  },
  "preferGlobal": true,
  "author": {
    "name": "Super Kenil",
    "url": "https://superkenil.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Super-Kenil/super-detype.git"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "chalk": "4.1.2",
    "cli-progress": "^3.12.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.3"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/fs-extra": "^11.0.4",
    "esbuild": "^0.25.12",
    "@types/cli-progress": "^3.11.6",
    "typescript": "^5.9.3"
  }
}
